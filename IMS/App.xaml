<Application x:Class="IMS.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:IMS"
             xmlns:vm="clr-namespace:IMS.ViewModels"
             xmlns:relicvm="clr-namespace:IMS.ViewModels"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>

                <ResourceDictionary Source="/IMS;component/Themes/Animations.xaml" />
                <ResourceDictionary Source="/IMS;component/Themes/Theme.xaml" />

                <ResourceDictionary Source="/IMS;component/Themes/Images.xaml" />
                <ResourceDictionary Source="/IMS;component/Themes/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
